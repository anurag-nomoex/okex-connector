{
	"source": {
		"include": [ "./src/modules/", "./package.json" ]
	},
	"plugins": [ "plugins/markdown" ],
	"templates": {
		"default": {
			"includeDate": false
		}
	},
	"opts": {
		"readme": "./docs_src/intro.md",
		"destination": "./docs",
		"template": "node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"title": "okx-broker-connector-node",
			"resizeable": {
				"navbar": {
					"min": "320",
					"max": "600"
				}
			},
			"menu": [
				{
					"title": "Changelog",
					"link": "./changelog.html"
				},
				{
					"title": "Getting Started",
					"link": "./gettingStarted.html"
				},
				{
					"title": "GitHub repository",
					"link": "https://gitlab.com/nomoex-backend/okx-broker-connector-node"
				}
			]
		}
	}
}
